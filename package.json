    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@prisma/client": "6.x.x",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "spacetime": "^7.12.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
