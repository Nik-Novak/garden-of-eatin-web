{
  "name": "garden-of-eatin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:steps": "yarn prisma generate && next build",
    "build": "dotenvx run -f .env.production -- yarn run build:steps",
    "migrate": "dotenvx run -f .env -- tsx ./prisma/migrations/index.ts",
    "dev": "dotenvx run -f .env -- next dev",
